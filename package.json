    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "chalk": "^2.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.1.1",
